package cn.com.nttdatabj.h2.uq.window.menu.tools;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import cn.com.nttdatabj.h2.uq.ipc.IPCWindow;

public class MenuItemIPCAction implements ActionListener {

	@Override
	public void actionPerformed(ActionEvent arg0) {
		// String buttonName = arg0.getActionCommand();
		Action();
		return;
	}

	private void Action() {
		IPCWindow ipcw = new IPCWindow();
		ipcw.setVisible(true);
		return;
	}
}
